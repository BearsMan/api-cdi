            "type": "string",
            "enum": [
              "Pve",
              "Dungeon",
              "Wvw",
              "Activity",
              "PvpLobby",
              "Pvp"
            ]
          }
        },
        "flags": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "NoSell",
              "SoulBindOnUse",
              "AccountBound",
              "AccountBindOnUse",
              "NoSalvage",
              "HideSuffix",
              "NoMysticForge",
              "SoulbindOnAcquire",
              "DeleteWarning",
              "NotUpgradeable",
              "Unique",
              "Tonic",
              "NoUnderwater",
              "BulkConsume",
              "Infused",
              "Attuned",
              "MonsterOnly"
            ]
          }
        },
        "restrictions": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "Asura",
              "Charr",
              "Human",
              "Norn",
              "Sylvari",
              "Warrior",
              "Necromancer",
              "Elementalist",
              "Guardian",
              "Thief",
              "Ranger",
              "Mesmer",
              "Engineer",
              "Female"
            ]
          }
        },
        "id": {
          "type": "integer",
          "example": 1
        },
        "chat_link": {
          "type": "string",
          "example": "[&AgEBAAAA]"
        },
        "icon": {
          "type": "string",
          "example": "https://render.guildwars2.com/file/935CD7A5037BB9F3C46E0D770D2DD9730ED74A18/62495.png"
        },
        "details": {
          "$ref": "#/definitions/Details"
        },
        "description": {
          "type": "string",
          "example": "A mysteriously light and well-balanced weapon that seems malleable to your wishes. Double-click to choose what form it should take."
        },
        "upgrades_into": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Upgrades"
          }
        },
        "upgrades_from": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Upgrades"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "name",
        "type",
        "level",
        "rarity",
        "vendor_value",
        "game_types",
        "flags",
        "restrictions",
        "id",
        "chat_link"
      ]
