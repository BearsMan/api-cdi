{
  "type": "array",
  "items": {
    "anyOf": [
      {
        "type": "integer"
      },
      {
        "$ref": "#/definitions/Item"
      }
    ]
  },
  "definitions": {
    "Item": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "example": "Fallen Adventurer's Backpack"
        },
        "type": {
          "type": "string",
          "enum": [
            "Weapon",
            "Armor",
            "Consumable",
            "Container",
            "Trophy",
            "Trinket",
            "CraftingMaterial",
            "UpgradeComponent",
            "Gizmo",
            "MiniPet",
            "Back",
            "Trait",
            "Bag",
            "Gathering",
            "Key",
            "Tool"
          ]
        },
        "level": {
          "type": "integer",
          "example": 80
        },
        "rarity": {
          "type": "string",
          "enum": [
            "Exotic",
            "Rare",
            "Masterwork",
            "Fine",
            "Basic",
            "Ascended",
            "Legendary",
            "Junk"
          ]
        },
        "vendor_value": {
          "type": "integer",
          "example": 264
        },
        "default_skin": {
          "type": "integer",
          "example": 5091
        },
        "game_types": {
          "type": "array",
